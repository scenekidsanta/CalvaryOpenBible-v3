</head>

<body style= "padding-top:400px;">
    <header style=" background-image:url('../../Content/Images/M__EFE0.jpg'); background-size:cover;"></header>
    <div style=" background-image:url(../../Content/background_clean_1680x1050.jpg); background-size:cover;"></div>
    <div class="navbar navbar-inverse main-nav" style="align-content:center">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>

            </div>
            <div id="menu" class="navbar-collapse collapse">
                <ul id="nav" class="nav navbar-nav" style="">
                    <li style="height: 83px;">@Html.ActionLink("Home", "Index", "Home")</li>
                    <li style="height: 83px;">
                        @Html.ActionLink("About", "About", "Home")
                        <ul class="nav navbar-nav navbar navbar-inverse" style="margin-top: -17px; width: 190px; padding-left: 15px;">
                            <li><a href="~/Pastors/Index/">Pastors</a></li>
                            <li><a href="~/Beliefs/Index/">Beliefs</a></li>
                        </ul>
                    </li>
                    <li style="height: 83px;">
                        @Html.ActionLink("Ministries", "Index", "Ministries")
                        <ul class="nav navbar-nav navbar navbar-inverse" style="margin-top: -17px; width:650px; padding-left: 15px;">
                            <li><a href="~/Ministries/HighSchool">High School</a></li>
                            <li><a href="~/Ministries/MiddleSchool">Middle School</a></li>
                            <li><a href="~/Ministries/Children">Children</a></li>
                            <li>
                                <a href="#" style=" height: 51px;">Community Groups</a>
                                <ul class="nav navbar-nav navbar navbar-inverse" style="margin-top: 51px; padding-left: 15px; width: 200px;">
                                    <li><a href="~/Ministries/College/">College</a></li>
                                    <li><a href="~/Ministries/Adults/">Adults</a></li>
                                </ul>
                            </li>
                            <li><a href="~/Ministries/Seniors/">Seniors</a></li>
                        </ul>
                    </li>
                    <li style="height: 83px;">
                        @Html.ActionLink("Media", "Media", "Home")
                        <ul class="nav navbar-nav navbar navbar-inverse" style="margin-top: -17px; padding-left: 15px;">
                            <li><a href="~/Images/LiveStream/">Live Stream</a></li>
                            <li><a href="~/Images/Index/">Photos</a></li>
                            <li><a href="~/Images/LiveStream/">Videos</a></li>
                        </ul>
                    </li>
                    <li style="height: 83px;">
                    <li style="height: 83px;">
                        @Html.ActionLink("Calendar", "Calendar", "Event")
                        <ul class="nav navbar-nav navbar navbar-inverse" style="margin-top: -17px; width: 90px; padding-left: 15px;">
                            <li><a href="~/Event/Index">Events</a></li>
                        </ul>
                </li>
              
                <li style="height: 83px;">@Html.ActionLink("Contact", "Contact", "Home")</li>
                </ul>

                @* @Html.Partial("_LoginPartial")*@

</div>
        </div>
    </div>
    <div style="position:absolute; padding-top: 390px; width: 100%; background:url(../../Content/background_clean_1680x1050.jpg);  background-size:cover;  float: left;">
        <br /><br /><br /><br /><br /><br /><br /><br /><br /><br /><br /><br />
        <br /><br /><br /><br /><br /><br /><br /><br /><br /><br /><br /><br />
        <br /><br /><br /><br /><br /><br /><br /><br /><br /><br /><br /><br />
    </div>
    <!--<div style="position:absolute; padding-top: 390px; width: 25%; background-color: #FFFFFF; margin-left: 75%;">
        <br /><br /><br /><br /><br /><br /><br /><br /><br /><br /><br /><br />
        <br /><br /><br /><br /><br /><br /><br /><br /><br /><br /><br /><br />
        <br /><br /><br /><br /><br /><br /><br /><br /><br /><br /><br /><br />
    </div>-->
    <div class="container body-content main" style="background-color : transparent;width: 90%;">
        @RenderBody()
       
        <footer style="margin-top:2%;margin-left:75%; color:white;">
            <p>&copy; @DateTime.Now.Year - Calvary Open Bible</p>
        </footer>
    </div>
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)

</body>


</html>